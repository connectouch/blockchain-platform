{
  "name": "connectouch-defi-ai",
  "version": "1.0.0-alpha",
  "description": "AI-Powered DeFi Intelligence Platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd connectouch-modern && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd connectouch-modern && npm run build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd connectouch-modern && npm test",
    "test:ai": "cd backend && npm run test:ai",
    "install:all": "npm install && cd backend && npm install && cd ../connectouch-modern && npm install",
    "clean": "rm -rf node_modules backend/node_modules connectouch-modern/node_modules",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "start": "node working-api-server.js",
    "setup": "npm run install:all"
  },
  "keywords": [
    "defi",
    "ai",
    "blockchain",
    "web3",
    "ethereum",
    "openai",
    "yield-farming",
    "smart-contracts",
    "dapp",
    "fintech"
  ],
  "author": "Connectouch Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/connectouch/connectouch-defi-ai.git"
  },
  "bugs": {
    "url": "https://github.com/connectouch/connectouch-defi-ai/issues"
  },
  "homepage": "https://connectouch.ai",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "openai": "^5.9.0",
    "socket.io-client": "^4.8.1"
  }
}
