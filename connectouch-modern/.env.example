# Connectouch Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_WS_URL=ws://localhost:3001

# OpenAI Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Blockchain Configuration
REACT_APP_ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_project_id
REACT_APP_POLYGON_RPC_URL=https://polygon-mainnet.infura.io/v3/your_project_id
REACT_APP_BSC_RPC_URL=https://bsc-dataseed.binance.org/
REACT_APP_AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc
REACT_APP_ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
REACT_APP_OPTIMISM_RPC_URL=https://mainnet.optimism.io

# Analytics
REACT_APP_ANALYTICS_ID=your_analytics_id
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Feature Flags
REACT_APP_ENABLE_AI_ASSISTANT=true
REACT_APP_ENABLE_VOICE_COMMANDS=true
REACT_APP_ENABLE_TRADING=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# Development
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info
