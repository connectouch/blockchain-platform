{
  "version": 2,
  "buildCommand": "cd apps/frontend && npm install && npm run build:production",
  "outputDirectory": "apps/frontend/dist",
  "installCommand": "cd apps/frontend && npm install --legacy-peer-deps",
  "devCommand": "cd apps/frontend && npm run dev",
  "framework": "vite",

  "rewrites": [
    {
      "source": "/api/health-check",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/health-check"
    },
    {
      "source": "/api/crypto-prices",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/crypto-prices"
    },
    {
      "source": "/api/live-prices",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/live-prices"
    },
    {
      "source": "/api/nft-collections",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/nft-collections"
    },
    {
      "source": "/api/ai-analysis",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/ai-analysis"
    },
    {
      "source": "/api/ai-predictions",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/ai-predictions"
    },
    {
      "source": "/api/defi-protocols",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/defi-protocols"
    },
    {
      "source": "/api/portfolio-sync",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/portfolio-sync"
    },
    {
      "source": "/api/image-proxy",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/image-proxy"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://aompecyfgnakkmldhidg.supabase.co/functions/v1/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-Powered-By",
          "value": "Connectouch Blockchain AI Platform"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "VITE_SUPABASE_URL": "https://aompecyfgnakkmldhidg.supabase.co",
    "VITE_SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImFvbXBlY3lmZ25ha2ttbGRoaWRnIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NTM3MzMwODYsImV4cCI6MjA2OTMwOTA4Nn0.SSbGerxCplUZd_ZJDCK3HrfHM_m0it2lExgKBv3bt9A",
    "VITE_APP_NAME": "Connectouch Blockchain AI Platform",
    "VITE_APP_VERSION": "2.0.0",
    "VITE_ENABLE_AI_ASSISTANT": "true",
    "VITE_ENABLE_VOICE_COMMANDS": "true",
    "VITE_ENABLE_TRADING": "true",
    "VITE_ENABLE_NOTIFICATIONS": "true",
    "VITE_ENABLE_REAL_TIME_DATA": "true",
    "VITE_ENABLE_ADVANCED_CHARTS": "true",
    "VITE_ENABLE_MULTI_CHAIN": "true",
    "VITE_ENABLE_PORTFOLIO_ANALYTICS": "true",
    "VITE_ENABLE_GAMEFI": "true",
    "VITE_ENABLE_DAO_FEATURES": "true",
    "VITE_ENABLE_INFRASTRUCTURE_MONITORING": "true"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "VITE_BUILD_TARGET": "production"
    }
  }
}
